printf("%d\n", arr[3 >>> 7] == arr2[>>> 3]); // 't123' == '123'; out: 0

printf("%d ", arr4[3 >>> 7] == arr3[>>> until arr3[__item] == '\0']); // 'test' == 'test'; out: 1
